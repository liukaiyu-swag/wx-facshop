/* pages/discover/discover.wxss */

navigator {
  box-sizing: border-box;
}

page {
  background-color: #f6f6f6;
}

.ul {
  margin-top: 30rpx;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  padding-left: 30rpx;
}

.li {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #e5e5e5;
  padding-right: 30rpx;
}

.li:first-child {
  border-top: none;
}

.li .icon {
  float: left;
  width: 80rpx;
  height: 100%;
  background: no-repeat center center/ 40rpx auto;
}

.icon.hy {
  background-image: url(data:image/png;base64,);
}

.icon.sj {
  background-image: url(data:image/png;base64,);
}

.icon.kh {
  background-image: url(data:image/png;base64,);
}

.icon.wl {
  background-image: url(data:image/png;base64,);
}

.li .text {
  padding-left: 30rpx;
  float: left;
}

.li .enter-icon {
  background: url(data:image/png;base64,) no-repeat center center / 15rpx auto;
  float: right;
  width: 60rpx;
  height: 100%;
}